{
  "name": "vite-ssr-vue",
  "description": "Vite utility for vue3 server side rendering",
  "version": "0.0.1",
  "main": "./src/vue/client.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "lint": "eslint --ext .ts --ignore-path .gitignore ./src/**/*",
    "build:cjs": "cd src && yarn tsc",
    "build:esm": "cd src && yarn tsc --project tsconfig-esm.json"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.4.2",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.29.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.8",
    "vue": "^3.0.5",
    "vue-router": "^4.0.10",
    "vue-tsc": "^0.0.24",
    "vuex": "^4.0.2",
    "@vueuse/head": "^0.6.0"
  },
  "license": "MIT",
  "keywords": [
    "vite",
    "ssr",
    "vue",
    "vue3"
  ],
  "author": "webigorkiev",
  "workspaces": [
    "example"
  ],
  "bugs": {
    "url": "https://github.com/webigorkiev/vite-ssr-vue/issues"
  },
  "homepage": "https://github.com/webigorkiev/vite-ssr-vue#readme"
}
